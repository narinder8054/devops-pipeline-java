pipeline{
    agent any
    environment {
      JAVA_VERSION="11"
      DEVOPS_BATCH="AUG2022"
    }
    tools {
      jdk 'jdk11'
      maven 'maven'
    }

    stages {
      stage('checkout'){
        steps {
            checkout scm
        }
      }
      stage('compile'){
        steps {
            sh 'mvn compile'
        }
      }

    }
}